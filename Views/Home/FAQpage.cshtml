    <nav class="nav-extended">
        <div class="nav-wrapper">
            <div>
                <a id="infinity">Infinity</a>
                <a id="prints">Prints</a>
            </div>
            <div class="nav-content">
                <ul class="tabs">
                    <li class="tab"><a ng-click="Homepage()">Home</a></li>
                    <li class="tab"><a ng-click="Servicepage()">Services</a></li>
                    <li class="tab"><a ng-click="Reviewpage()">Reviews</a></li>
                    <li class="tab"><a ng-click="OrderPage()">Order</a></li>
                    <li class="tab"><a ng-click="FAQpage()" class="active">FAQs</a></li>
                    <li class="tab"><a ng-click="AboutusPage()">About Us</a></li>
                    <li class="tab"><a ng-click="SignIn()">{{Name}}</a></li>

                </ul>
                <div class="right-menu">
                    <div class="menu-toggle" id="menu-toggle">&#9776;</div>
                </div>
            </div>
        </div>

