@{
    Layout = "~/Views/Shared/_HomepageLayout.cshtml";
}


<style>
    .guide-container {
        background-color: #6f0d4a;
        border-top-right-radius: 100px;
        border-top-left-radius: 0px;
        border-bottom-right-radius: 0px;
        border-bottom-left-radius: 0px;
        padding-left: 40px;
        padding-right: 20px;
        padding-top: 30px;
        padding-bottom: 50px;
        width: 90%;
        margin-top: 50px;
        margin-bottom: 50px;
        color: white;
        position: relative;
        box-shadow: 10px 10px 20px rgba(0, 0, 0, 0.3); /* Box shadow added */
    }

    .faq-container {
        background-color: #6f0d4a;
        border-top-right-radius: 0px;
        border-top-left-radius: 0px;
        border-bottom-right-radius: 100px;
        border-bottom-left-radius: 0px;
        padding-left: 40px;
        padding-right: 20px;
        padding-top: 30px;
        padding-bottom: 20px;
        width: 90%;
        margin-top: 50px;
        margin-bottom: 50px;
        color: white;
        position: relative;
        box-shadow: 10px 10px 20px rgba(0, 0, 0, 0.3); /* Box shadow added */
    }

    .text-block {
        max-width: 100%;
        text-align: justify;
        margin-left: 10px;
        margin-right: 20px;
    }

    .guide-label .col {
        display:inline-block
    }

    .row {
        border-left: 40px double white;
    }

    h2 {
        font-weight: 0 !important;
        line-height: 1.3;
        margin: 0px !important;
        font-weight:bolder
    }
</style>
